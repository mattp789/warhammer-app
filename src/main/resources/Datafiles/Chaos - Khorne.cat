    <selectionEntry id="26f1-73f3-b27b-b3ad" name="Battle Tactics: For the Skull Throne" publicationId="a4ca-1bc7-0120-7e88" hidden="false" collective="false" import="true" type="upgrade">
      <profiles>
        <profile id="1b19-ad14-3d90-47bc" name="Blood for the Altar" publicationId="a4ca-1bc7-0120-7e88" hidden="false" typeId="eba0-4bf6-65fe-fdbc" typeName="Battle Tactic">
          <characteristics>
            <characteristic name="Description" typeId="f882-48d5-21a4-1787">Pick 1 enemy unit within 8&quot; of your Skull Altar. You complete this battle tactic if that unit is destroyed during this turn.</characteristic>
          </characteristics>
        </profile>
        <profile id="b896-e7a0-26cb-37e5" name="Slay the Sorcerer" publicationId="a4ca-1bc7-0120-7e88" hidden="false" typeId="eba0-4bf6-65fe-fdbc" typeName="Battle Tactic">
          <characteristics>
            <characteristic name="Description" typeId="f882-48d5-21a4-1787">Pick 1 enemy WIZARD HERO on the battlefield. You complete this battle tactic if that HERO is slain during this turn.</characteristic>
          </characteristics>
        </profile>
        <profile id="3bb1-5f94-a544-0e1c" name="The Trial of Skulls" publicationId="a4ca-1bc7-0120-7e88" hidden="false" typeId="eba0-4bf6-65fe-fdbc" typeName="Battle Tactic">
          <characteristics>
            <characteristic name="Description" typeId="f882-48d5-21a4-1787">Pick 1 friendly unit. You complete this battle tactic if 8 or more enemy models are slain by attacks made by that unit during this turn.</characteristic>
          </characteristics>
        </profile>
        <profile id="f670-5256-572e-5184" name="The Battlefield Runs Red" hidden="false" typeId="eba0-4bf6-65fe-fdbc" typeName="Battle Tactic">
          <characteristics>
            <characteristic name="Description" typeId="f882-48d5-21a4-1787">You complete this battle tactic if 4 or more units were destroyed during this turn.</characteristic>
          </characteristics>
        </profile>
        <profile id="1186-8b40-13f9-04e6" name="Leave None Standing" hidden="false" typeId="eba0-4bf6-65fe-fdbc" typeName="Battle Tactic">
          <characteristics>
            <characteristic name="Description" typeId="f882-48d5-21a4-1787">Pick 1 friendly unit within 3&quot; of any enemy units.  You complete this battle tactic if that unit fights in the combat phase of this turn and at the end of that phase there are no enemy units within 3&quot; of that unit.</characteristic>
          </characteristics>
        </profile>
        <profile id="0ccf-57f5-5ada-dff6" name="No Cowards Among Us" hidden="false" typeId="eba0-4bf6-65fe-fdbc" typeName="Battle Tactic">
          <characteristics>
            <characteristic name="Description" typeId="f882-48d5-21a4-1787">You complete this battle tactic at the end of this turn if all friendly BLADES OF KHORNE units on the battlefield are within 8&quot; of any enemy units.</characteristic>
          </characteristics>
        </profile>
      </profiles>
