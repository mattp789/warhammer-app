            <characteristic name="Spells Known" typeId="dc9c-47d3-6931-859c">Arcane Bolt, Mystic Shield and Traitor&apos;s Mist.</characteristic>
          </characteristics>
        </profile>
        <profile id="dade-ea77-71ee-9a69" name="Traitor&apos;s Mist" hidden="false" typeId="2e81-5e22-c6e1-73cb" typeName="Spell">
          <characteristics>
