        <selectionEntry id="913e-209e-d0b4-9098" name="Glowy Shield of Protectiness" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="45a2-6cd4-0cea-0a9d" type="max"/>
