        <selectionEntry id="a382-8c1f-e5c4-dbff" name="The Shatterer" hidden="false" collective="false" import="true" type="upgrade">
          <constraints>
            <constraint field="selections" scope="parent" value="1" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="9250-89f5-e464-1807" type="max"/>
